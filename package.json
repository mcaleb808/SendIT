{
  "name": "newapi",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "test": "npm run build && npm run testbuild && jasmine spec/parcelSpec.js",
    "testbuild": "babel test/ -d spec && babel --presets es2015 test/ -d spec",
    "build": "babel src/ -d dist && babel --presets es2015 src/ -d dist",
    "start": "npm run build && npm run testbuild && node dist/server.js",
    "coveralls": "istanbul cover node_modules/jasmine/bin/jasmine.js && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "test-cover": "istanbul cover node_modules/jasmine/bin/jasmine.js && istanbul check-coverage"  
  },
  "dependencies": {
    "express": "^4.16.4",
    "joi": "^13.1.0",
    "nodemon": "^1.18.6",
    "pg": "^7.6.1",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "request": "^2.88.0"
  },
  "devDependencies": {
    "babel-watch": "^2.0.7",
    "coveralls": "^3.0.2",
    "istanbul": "^0.4.5",
    "jasmine": "^3.3.0",
    "jasmine-console-reporter": "^3.1.0",
    "jasmine-node": "^1.16.2",
    "jasmine-supertest": "^1.0.0",
    "supertest": "^3.3.0"
  },
